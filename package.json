{
  "name": "unocss-unit-to-vw",
  "type": "module",
  "version": "0.0.3",
  "description": "Convert all rem/px to vw in utils",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "keywords": [
    "unocss",
    "rem/px to vw",
    "preset"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "play": "pnpm -C playground run dev"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@unocss/core": "^0.50.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@types/node": "^18.14.1",
    "bumpp": "^9.0.0",
    "eslint": "^8.34.0",
    "fast-glob": "^3.2.12",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  }
}
