{
  "name": "unocss-unit-to-vw",
  "type": "module",
  "version": "0.0.0",
  "description": "Convert all rem/px to vw in utils",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "keywords": [
    "unocss",
    "rem-to-vw",
    "preset"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "play":"pnpm -C playground run dev"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@unocss/core": "^0.49.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.1",
    "@types/node": "^18.11.19",
    "bumpp": "^8.2.1",
    "eslint": "^8.33.0",
    "fast-glob": "^3.2.12",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  }
}
